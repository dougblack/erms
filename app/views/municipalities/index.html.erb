<h1>Listing municipalities</h1>

<table>
  <tr>
    <th>Username</th>
    <th>Population</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @municipalities.each do |municipality| %>
  <tr>
    <td><%= municipality.username %></td>
    <td><%= municipality.population %></td>
    <td><%= link_to 'Show', municipality %></td>
    <td><%= link_to 'Edit', edit_municipality_path(municipality) %></td>
    <td><%= link_to 'Destroy', municipality, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Municipality', new_municipality_path %>
